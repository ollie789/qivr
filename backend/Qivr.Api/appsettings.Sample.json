{
  "Branding": {
    "AllowedEmbedOrigins": [
      "https://clinic1.example.com",
      "https://clinic2.example.com",
      "https://partner.healthcare.com",
      "https://*.trusted-clinics.com"
    ]
  },
  "Features": {
    "EnableAsyncProcessing": true,
    "ProcessIntakeQueue": true,
    "EnableAiAnalysis": false,
    "SendEmailNotifications": true
  },
  "Sqs": {
    "QueueUrl": "https://sqs.ap-southeast-2.amazonaws.com/123456789/qivr-intake-queue",
    "Region": "ap-southeast-2",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 20,
    "VisibilityTimeout": 300
  },
  "Intake": {
    "ConnectionString": "${INTAKE_DATABASE_URL}"
  },
  "Security": {
    "DefaultTenantId": "00000000-0000-0000-0000-000000000001",
    "RequireHttps": true
  },
  "Notifications": {
    "BusinessHoursStartLocal": 9,
    "BusinessHoursEndLocal": 18,
    "EnforceQuietHours": true,
    "DefaultTimeZone": "Australia/Sydney"
  },
  "MessageMedia": {
    "ApiKey": "${MESSAGEMEDIA_API_KEY}",
    "ApiSecret": "${MESSAGEMEDIA_API_SECRET}",
    "WebhookSecret": "${MESSAGEMEDIA_WEBHOOK_SECRET}"
  }
}
