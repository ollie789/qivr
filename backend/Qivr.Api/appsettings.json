{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "UseJwtAuth": true,
  "UseMockAuth": false,
  "Jwt": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "qivr.health",
    "Audience": "qivr-api",
    "ExpiryMinutes": 60
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=qivr;Username=qivr_user;Password=${QIVR_DB_PWD}"
  },
  "Intake": {
    "ConnectionString": "Host=localhost;Port=5432;Database=qivr;Username=qivr_intake;Password=${INTAKE_DB_PWD}"
  },
  "Sqs": {
    "QueueUrl": "${SQS_QUEUE_URL}",
    "Region": "ap-southeast-2",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 20,
    "VisibilityTimeout": 300
  },
  "Features": {
    "ProcessIntakeQueue": true,
    "EnableAiAnalysis": false,
    "SendEmailNotifications": false,
    "EnableAsyncProcessing": true
  },
  "ApplyMigrations": true,
  "Cognito": {
    "Region": "ap-southeast-2",
    "UserPoolId": "${COGNITO_USER_POOL_ID}",
    "UserPoolClientId": "${COGNITO_CLIENT_ID}",
    "UserPoolClientSecret": "${COGNITO_CLIENT_SECRET}",
    "UserPoolDomain": "qivr-auth",
    "IdentityPoolId": "",
    "SamlMetadataUrl": "",
    "OidcProviderUrl": "",
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 30,
    "IdTokenExpirationMinutes": 60,
    "MfaEnabled": false,
    "MfaMethods": ["SMS", "TOTP"],
    "Google": {
      "ClientId": "",
      "ClientSecret": "",
      "Scopes": ["openid", "profile", "email"]
    },
    "Facebook": {
      "AppId": "",
      "AppSecret": "",
      "Scopes": ["public_profile", "email"]
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:3001",
      "http://localhost:3002"
    ]
  },
  "Email": {
    "Provider": "mailhog",
    "SmtpHost": "localhost",
    "SmtpPort": 1025,
    "SmtpUsername": "",
    "SmtpPassword": "",
    "SmtpUseSsl": false,
    "FromEmail": "noreply@qivr.health",
    "FromName": "Qivr Health",
    "Enabled": true
  },
  "App": {
    "BaseUrl": "http://localhost:3002"
  },
  "Smtp": {
    "Host": "localhost",
    "Port": 1025,
    "FromEmail": "noreply@qivr.health",
    "FromName": "Qivr Health"
  },
  "MessageMedia": {
    "ApiKey": "${MESSAGE_MEDIA_API_KEY}",
    "ApiSecret": "${MESSAGE_MEDIA_API_SECRET}",
    "FromNumber": "+61400000000"
  },
  "S3": {
    "Endpoint": "http://localhost:9000",
    "AccessKey": "${S3_ACCESS_KEY}",
    "SecretKey": "${S3_SECRET_KEY}",
    "BucketName": "qivr-uploads"
  },
  "OpenTelemetry": {
    "Endpoint": "http://localhost:4317"
  },
  "Environment": "Development",
  "Security": {
    "AllowedClinicDomains": [
      "*.clinic.local",
      "*.qivr.health",
      "localhost:*"
    ],
    "DefaultTenantId": "00000000-0000-0000-0000-000000000001",
    "EnableSecurityHeaders": true,
    "RequireHttps": false,
    "CspReportUri": "/api/security/csp-report"
  }
}
