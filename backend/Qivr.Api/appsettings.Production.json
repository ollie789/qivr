{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Qivr": "Information"
    }
  },
  "UseJwtAuth": true,
  "UseMockAuth": false,
  "Jwt": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "ExpiryMinutes": 60
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${CONNECTION_STRING}"
  },
  "Intake": {
    "ConnectionString": "${INTAKE_CONNECTION_STRING}"
  },
  "Sqs": {
    "QueueUrl": "${SQS_QUEUE_URL}",
    "Region": "${AWS_REGION}",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 20,
    "VisibilityTimeout": 300
  },
  "Features": {
    "ProcessIntakeQueue": true,
    "EnableAiAnalysis": true,
    "SendEmailNotifications": true,
    "EnableAsyncProcessing": true
  },
  "ApplyMigrations": false,
  "Cognito": {
    "Region": "${AWS_REGION}",
    "UserPoolId": "${COGNITO_USER_POOL_ID}",
    "UserPoolClientId": "${COGNITO_CLIENT_ID}",
    "UserPoolClientSecret": "${COGNITO_CLIENT_SECRET}",
    "UserPoolDomain": "${COGNITO_DOMAIN}",
    "IdentityPoolId": "${COGNITO_IDENTITY_POOL_ID}",
    "AccessTokenExpirationMinutes": 15,
    "RefreshTokenExpirationDays": 30,
    "IdTokenExpirationMinutes": 60,
    "MfaEnabled": true,
    "MfaMethods": ["SMS", "TOTP"]
  },
  "Cors": {
    "AllowedOrigins": "${CORS_ALLOWED_ORIGINS}"
  },
  "Email": {
    "Provider": "ses",
    "SmtpHost": "${SMTP_HOST}",
    "SmtpPort": 587,
    "SmtpUsername": "${SMTP_USERNAME}",
    "SmtpPassword": "${SMTP_PASSWORD}",
    "SmtpUseSsl": true,
    "FromEmail": "${FROM_EMAIL}",
    "FromName": "Qivr Health",
    "Enabled": true
  },
  "App": {
    "BaseUrl": "${APP_BASE_URL}"
  },
  "MessageMedia": {
    "ApiKey": "${MESSAGE_MEDIA_API_KEY}",
    "ApiSecret": "${MESSAGE_MEDIA_API_SECRET}",
    "FromNumber": "${MESSAGE_MEDIA_FROM_NUMBER}"
  },
  "S3": {
    "BucketName": "${S3_BUCKET_NAME}",
    "Region": "${AWS_REGION}"
  },
  "Bedrock": {
    "Region": "${AWS_REGION}",
    "AccessKey": "${BEDROCK_ACCESS_KEY}",
    "SecretKey": "${BEDROCK_SECRET_KEY}"
  },
  "OpenTelemetry": {
    "Endpoint": "${OTEL_EXPORTER_OTLP_ENDPOINT}"
  },
  "Environment": "Production",
  "Security": {
    "AllowedClinicDomains": "${ALLOWED_CLINIC_DOMAINS}",
    "DefaultTenantId": "${DEFAULT_TENANT_ID}",
    "EnableSecurityHeaders": true,
    "RequireHttps": true,
    "CspReportUri": "/api/security/csp-report"
  },
  "RateLimiting": {
    "EnableRateLimiting": true,
    "PermitLimit": 100,
    "Window": "00:01:00",
    "QueueProcessingOrder": "OldestFirst",
    "QueueLimit": 50
  },
  "DataProtection": {
    "ApplicationName": "Qivr",
    "KeyRingPath": "/app/keys"
  }
}
