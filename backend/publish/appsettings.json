{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "UseJwtAuth": true,
  "UseMockAuth": false,
  "Jwt": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "qivr.health",
    "Audience": "qivr-api",
    "ExpiryMinutes": 60
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${DEFAULT_CONNECTION}"
  },
  "Intake": {
    "ConnectionString": "${INTAKE_CONNECTION}"
  },
  "Sqs": {
    "QueueUrl": "${SQS_QUEUE_URL}",
    "Region": "ap-southeast-2",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 20,
    "VisibilityTimeout": 300
  },
  "Features": {
    "ProcessIntakeQueue": true,
    "EnableAiAnalysis": false,
    "SendEmailNotifications": false,
    "EnableAsyncProcessing": true
  },
  "ApplyMigrations": true,
  "Cors": {
    "AllowedOrigins": []
  },
  "Email": {
    "Provider": "${EMAIL_PROVIDER}",
    "SmtpHost": "${EMAIL_SMTP_HOST}",
    "SmtpPort": "${EMAIL_SMTP_PORT}",
    "SmtpUsername": "${EMAIL_SMTP_USERNAME}",
    "SmtpPassword": "${EMAIL_SMTP_PASSWORD}",
    "SmtpUseSsl": "${EMAIL_SMTP_USE_SSL}",
    "FromEmail": "${EMAIL_FROM_EMAIL}",
    "FromName": "${EMAIL_FROM_NAME}",
    "Enabled": "${EMAIL_ENABLED}"
  },
  "App": {
    "BaseUrl": "${APP_BASE_URL}"
  },
  "Smtp": {
    "Host": "${SMTP_HOST}",
    "Port": "${SMTP_PORT}",
    "FromEmail": "${SMTP_FROM_EMAIL}",
    "FromName": "${SMTP_FROM_NAME}"
  },
  "MessageMedia": {
    "ApiKey": "${MESSAGE_MEDIA_API_KEY}",
    "ApiSecret": "${MESSAGE_MEDIA_API_SECRET}",
    "FromNumber": "${MESSAGE_MEDIA_FROM_NUMBER}"
  },
  "S3": {
    "Endpoint": "${S3_ENDPOINT}",
    "AccessKey": "${S3_ACCESS_KEY}",
    "SecretKey": "${S3_SECRET_KEY}",
    "BucketName": "${S3_BUCKET_NAME}"
  },
  "OpenTelemetry": {
    "Endpoint": "${OTEL_EXPORTER_OTLP_ENDPOINT}"
  },
  "Security": {
    "AllowedClinicDomains": [
      "*.qivr.health"
    ],
    "DefaultTenantId": "${DEFAULT_TENANT_ID}",
    "EnableSecurityHeaders": true,
    "RequireHttps": true,
    "CspReportUri": "/api/security/csp-report"
  }
}
